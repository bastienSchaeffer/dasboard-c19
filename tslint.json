{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "rules": {
    "no-any": true,
    "interface-name": false,
    "no-duplicate-imports": true,
    "no-var-requires": false,
    "ordered-imports": [
      true,
      {
        "import-sources-order": "lowercase-last",
        "named-imports-order": "lowercase-first",
        "grouped-imports": true
      }
    ]
  }
}